{
  "dataSourceConfig": "backend/src/api/data-source/config.json",
  "outDir": "backend/src/api/routes",
  "routeConfig": "backend/src/api/routes/config.json",
  "availables": {
    "web_block": {
      "overwrite": true,
      "useAuthenticatedRouter": true,
      "enableStateRoute": true,

      "***@1": "UPLOAD OPTIONS ***",

      "enableUpload": true,
      "uploadPath": "",
      "uploadField": "previewImage",

      "***@2": "FORM VALIDATION OPTIONS ***",

      "enableValidation": true,

      "***@3": "SERVE INDEX OPTIONS ***",

      "enableServeIndex": true,
      "serveIndexStaticPath": "",
      "serveIndexRoutePath": "",

      "***@4": "CLIENT OPTIONS ***",
      "enableGoToPage": false,
      "enableGoToLastPage": false,
      "enableListState": false,

      "generateComponent": true,
      "componentOutDir": "frontend/src/cp/components/cms-builder",
      "componentFormOutDir": "frontend/src/cp/components/cms-builder/form",
      "gridOptions": {},
      "pagerOptions": {},
      "routePath": "/builder/web-block-manager",
      "componentName": "WebBlockManager",
      "formName": "BlockForm",
      "itemName": "Block",
      "fieldItemName": "name",

      "***@5": "PARENT MODULE MODE OPTIONS Add access to sub module ***",
      "enableParentModule": false,
      "subModuleFk": "parent",
      "subModuleRoutePath": "",
      "subModuleItemName": "",

      "***@6": "SUB MODULE MODE OPTIONS Add access with parent module ***",
      "enableSubModule": true,
      "parentModuleRoutePath": "/builder/web-block-manager/template",
      "parentModuleFk": "templateId",
      "parentModuleItemName": "Template"
    }
  }
}
